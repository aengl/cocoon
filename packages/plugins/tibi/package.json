{
  "name": "@cocoon/plugin-tibi",
  "version": "0.101.0",
  "author": "Lynn Smeria <ae@cephea.de>",
  "files": [
    "dist/**/*",
    "!*.map",
    "!sourcemap-register.js"
  ],
  "cocoon": {
    "nodes": [
      "dist/index.js"
    ]
  },
  "dependencies": {
    "@cocoon/plugin-distance": "0.101.0"
  },
  "devDependencies": {
    "@cocoon/types": "0.101.0",
    "@sindresorhus/slugify": "0.9.1",
    "@zeit/ncc": "0.20.4",
    "gray-matter": "4.0.2",
    "prettier": "1.18.2"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "scripts": {
    "build": "yarn build:ncc",
    "build:ncc": "ncc build -m -e @cocoon/plugin-distance index.ts",
    "dev:ncc": "ncc build -msw -e @cocoon/plugin-distance index.ts",
    "dev:rollup": "exit"
  }
}

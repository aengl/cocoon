{
  "name": "@cocoon/plugin-google",
  "version": "0.101.0",
  "license": "GPL-3.0-or-later",
  "files": [
    "dist/**/*",
    "!*.map",
    "!sourcemap-register.js"
  ],
  "cocoon": {
    "nodes": [
      "dist/index.js"
    ]
  },
  "devDependencies": {
    "@zeit/ncc": "0.20.4",
    "googleapis": "42.0.0"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "scripts": {
    "build": "yarn build:ncc",
    "build:ncc": "ncc build -m index.js",
    "dev:ncc": "ncc build -msw index.js",
    "dev:rollup": "exit"
  }
}

{
  "name": "@cocoon/plugin-views",
  "version": "0.92.0",
  "license": "GPL-3.0-or-later",
  "main": "dist/index.js",
  "module": "dist/components.js",
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "@cocoon/rollup": "^0.92.0",
    "@cocoon/types": "^0.92.0",
    "@types/react-virtualized": "9.21.2",
    "@zeit/ncc": "0.20.4",
    "react-virtualized": "9.21.1"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "scripts": {
    "build": "yarn build:ncc && yarn build:rollup",
    "build:ncc": "ncc build -m -e lodash index.ts",
    "build:rollup": "rollup --config rollup.config.js",
    "dev:ncc": "ncc build -mw -e lodash index.ts",
    "dev:rollup": "DEBUG=1 rollup --config rollup.config.js --watch"
  }
}

{
  "name": "@cocoon/plugin-views",
  "version": "0.86.0",
  "license": "GPL-3.0-or-later",
  "main": "dist/index.js",
  "module": "dist/components.js",
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "@cocoon/rollup": "file:../../rollup",
    "@cocoon/types": "file:../../types",
    "@types/react-virtualized": "9.21.2",
    "@zeit/ncc": "0.18.5",
    "react-virtualized": "9.21.1"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "scripts": {
    "build": "yarn build:nodes && yarn build:components",
    "build:nodes": "ncc build -m -e lodash index.ts",
    "build:components": "rollup --config rollup.config.js",
    "dev:nodes": "ncc build -mw -e lodash index.ts",
    "dev:components": "DEBUG=1 rollup --config rollup.config.js --watch"
  }
}

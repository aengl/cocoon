{
  "name": "@cocoon/plugin-echarts",
  "version": "0.87.0",
  "license": "GPL-3.0-or-later",
  "main": "dist/index.js",
  "module": "dist/components.js",
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "@cocoon/rollup": "file:../../rollup",
    "@cocoon/types": "file:../../types",
    "@types/echarts": "4.1.9",
    "@zeit/ncc": "0.18.5",
    "css-element-queries": "1.2.0",
    "d3-array": "2.2.0",
    "echarts": "4.2.1"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "scripts": {
    "build": "yarn build:ncc && yarn build:rollup",
    "build:ncc": "ncc build -m index.ts",
    "build:rollup": "rollup --config rollup.config.js",
    "dev:ncc": "ncc build -mw index.ts",
    "dev:rollup": "DEBUG=1 rollup --config rollup.config.js --watch"
  }
}

{
  "name": "@cocoon/plugin-inspect",
  "version": "0.97.0",
  "license": "GPL-3.0-or-later",
  "main": "dist/index.js",
  "module": "dist/components.js",
  "files": [
    "dist/**/*",
    "!*.map",
    "sourcemap-register.js"
  ],
  "devDependencies": {
    "@babel/plugin-transform-runtime": "7.5.5",
    "@cocoon/rollup": "^0.97.0",
    "@cocoon/types": "^0.97.0",
    "@zeit/ncc": "0.20.4",
    "react-inspector": "3.0.2",
    "rollup-plugin-babel": "4.3.3"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "scripts": {
    "build": "yarn build:ncc && yarn build:rollup",
    "build:ncc": "ncc build -m index.ts",
    "build:rollup": "rollup --config rollup.config.js",
    "dev:ncc": "ncc build -msw index.ts",
    "dev:rollup": "DEBUG=1 rollup --config rollup.config.js --watch"
  }
}

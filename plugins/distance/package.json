{
  "name": "@cocoon/score",
  "version": "0.4.0",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/aengl/cocoon-score"
  },
  "license": "GPL-3.0-or-later",
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "@cocoon/testing": "0.1.0",
    "@cocoon/types": "0.4.1",
    "@types/d3-array": "2.0.0",
    "@types/d3-scale": "2.1.1",
    "@zeit/ncc": "0.18.5",
    "ava": "1.4.1",
    "d3-array": "2.2.0",
    "d3-scale": "3.0.0",
    "simple-statistics": "7.0.2",
    "ts-node": "8.2.0",
    "typescript": "3.4.5"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "scripts": {
    "build": "ncc build -m src/index.ts",
    "dev": "ncc build -w src/index.ts",
    "test": "ava -v",
    "test:watch": "ava -vw"
  }
}

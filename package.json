    "webpack-cli": "3.3.0",
    "webpack-dev-server": "3.2.1",
    "websocket-as-promised": "0.9.0",
    "ws": "6.2.1"
  },
  "devDependencies": {
    "nodemon": "1.18.10",
    "prettier": "1.16.4",
    "source-map-loader": "0.2.4",
    "ts-node": "8.0.3",
    "tslint": "5.14.0",
    "tslint-config-prettier": "1.18.0",
    "webpack-bundle-analyzer": "3.1.0"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "nodemonConfig": {
    "ignore": [
      "__tests__/*",
      "**/*.test.ts"
    ],
    "delay": "500"
  },
  "scripts": {
    "install": "scripts/install.sh",
    "start:cli": "node core/cli.js",
    "start:core": "node core/index.js",
    "start:editor": "node editor/bin.js",
    "start:server": "DEBUG=* webpack-dev-server",
    "clean": "scripts/clean.sh",
    "dev": "tsc && scripts/dev.js",
    "test": "ava -v",
    "test:watch": "ava -vw",
    "analyse-bundle": "scripts/analyse-bundle.sh"
  }
}

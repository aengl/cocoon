{
  "name": "cocoon2",
  "displayName": "Cocoon2",
  "version": "0.69.0",
  "author": "Lynn Smeria <ae@cephea.de>",
  "private": true,
  "bin": {
    "cocoon": "core/bin.js",
    "cocoon-editor": "editor/bin.js"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@sindresorhus/slugify": "0.9.1",
    "@tippy.js/react": "2.1.2",
    "@types/debug": "4.1.4",
    "@types/echarts": "4.1.9",
    "@types/got": "9.4.4",
    "@types/js-quantities": "1.6.2",
    "@types/js-yaml": "3.12.1",
    "@types/json-stable-stringify": "1.0.32",
    "@types/lodash": "4.14.123",
    "@types/mousetrap": "1.6.2",
    "@types/react": "16.8.15",
    "@types/react-dom": "16.8.4",
    "@types/react-virtualized": "9.21.1",
    "@types/styled-components": "4.1.14",
    "@types/webpack": "4.4.29",
    "@types/webpack-dev-server": "3.1.5",
    "@types/ws": "6.0.1",
    "ava": "1.4.1",
    "ayu": "7.2.1",
    "caporal": "1.1.0",
    "carlo": "0.9.45",
    "concurrently": "4.1.0",
    "css-element-queries": "1.1.1",
    "css-loader": "2.1.1",
    "debug": "4.1.1",
    "echarts": "4.2.1",
    "googleapis": "39.2.0",
    "got": "9.6.0",
    "gray-matter": "4.0.2",
    "isomorphic-ws": "4.0.1",
    "js-quantities": "1.7.3",
    "js-yaml": "3.13.1",
    "json-stable-stringify": "1.0.1",
    "lodash": "4.17.11",
    "monaco-editor": "0.16.2",
    "monaco-editor-webpack-plugin": "1.7.0",
    "mousetrap": "1.6.3",
    "opn": "6.0.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-draggable": "3.3.0",
    "react-splitter-layout": "4.0.0",
    "react-virtualized": "9.21.0",
    "serialize-error": "4.1.0",
    "simple-statistics": "7.0.2",
    "style-loader": "0.23.1",
    "styled-components": "4.2.0",
    "supports-color": "6.1.0",
    "tippy.js": "4.3.0",
    "tmp": "0.1.0",
    "type-fest": "0.4.1",
    "typescript": "3.4.5",
    "webpack": "4.30.0",
    "webpack-cli": "3.3.1",
    "webpack-dev-server": "3.3.1",
    "websocket-as-promised": "0.9.0",
    "ws": "7.0.0"
  },
  "devDependencies": {
    "nodemon": "1.19.0",
    "prettier": "1.17.0",
    "source-map-loader": "0.2.4",
    "ts-node": "8.1.0",
    "tslint": "5.16.0",
    "tslint-config-prettier": "1.18.0",
    "webpack-bundle-analyzer": "3.3.2"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "nodemonConfig": {
    "ignore": [
      "__tests__/*",
      "**/*.test.ts",
      "examples/*"
    ],
    "delay": "500"
  },
  "scripts": {
    "install": "tsc && webpack",
    "clean": "scripts/clean.sh",
    "cli": "node core/cli.js",
    "dev": "tsc && scripts/dev.js",
    "dev:watch": "tsc --watch",
    "dev:editor": "DEBUG=1 nodemon --inspect=9340 editor/main.js -- --headless",
    "dev:server": "DEBUG=1 webpack-dev-server",
    "test": "ava -v",
    "test:watch": "ava -vw",
    "analyse-bundle": "scripts/analyse-bundle.sh"
  }
}
